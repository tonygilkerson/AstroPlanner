APSF,V1,0,,-1,0,
// Copy a text file
//= Copy Text File

sub main()
	dim fin as APTextFile = APTextFile.ReadFile
	dim fout as APTextFile = nil
	if fin<>nil then
		fout = APTextFile.WriteFile
	end if
	
	if fin<>nil and fout<>nil then
		while not fin.EOF
			dim s as string = fin.ReadLine
			fout.WriteLine s
		wend
		fin.Close
		fout.Close
	end if
end sub
