		Deprecate("CatalogFound(n)","APCatalog.FoundObject(n)")
		return new APCatalogObject(n)
	end function

	function Choice(ParamArray s as string) as integer
		Deprecate("Choice","Dialog.Choice")
		return APC_Choice(s)
	end function

